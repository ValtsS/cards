{
  "name": "cards-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "ts-node ./server.ts",
    "preview": "vite preview",
    "lint": "eslint src --ext js,ts,tsx",
    "format": "prettier --write .",
    "test": "cross-env BABEL_ENV=test NODE_ENV=test jest",
    "watch-tests": "cross-env BABEL_ENV=test NODE_ENV=test jest --watch",
    "coverage": "cross-env BABEL_ENV=test NODE_ENV=test jest --coverage",
    "sonar": "/sonar/runsonar.sh",
    "pre-commit": "lint-staged",
    "gql:compile": "graphql-codegen",
    "gql:watch": "graphql-codegen -w",
    "check-strict": "eslint src --ext js,ts,tsx  --max-warnings 0 && tsc --noEmit --strict",
    "build:server": "vite build --outDir dist/server --ssr src/entry-server.tsx --config vite.server.config.ts",
    "build:server-prod": "tsc -p tsconfig.node.json",
    "prod": "cross-env NODE_ENV=production PORT=6999 node dist/server.js",
    "moveserver": "mv ./server.js ./dist/server.js",
    "build": "npm run lint && prettier --check . && tsc -noEmit && vite build && npm run build:server && npm run build:server-prod && npm run moveserver"
  },
  "dependencies": {
    "@apollo/client": "^3.7.12",
    "@reduxjs/toolkit": "^1.9.3",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.10.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.21.0",
    "@babel/plugin-transform-typescript": "^7.21.3",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.4",
    "@cypress/code-coverage": "^3.10.4",
    "@graphql-codegen/cli": "^3.3.0",
    "@graphql-codegen/client-preset": "^3.0.0",
    "@graphql-codegen/introspection": "^3.0.1",
    "@graphql-codegen/typescript-graphql-request": "^4.5.9",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/express": "^4.17.17",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "@vitejs/plugin-react": "^3.1.0",
    "babel-plugin-istanbul": "^6.1.1",
    "cross-env": "^7.0.3",
    "cypress": "^12.10.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-redux": "^4.0.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-cli": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-fail-on-console": "^3.1.1",
    "jest-junit": "^15.0.0",
    "lint-staged": "^13.2.1",
    "prettier": "^2.8.7",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "vite": "^4.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
